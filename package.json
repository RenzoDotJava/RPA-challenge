  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/react-native": "^12.0.0-rc.2",
    "@types/jest": "^29.4.0",
    "@types/react": "~18.0.14",
    "jest": "^29.4.3",
    "jest-expo": "^48.0.1",
